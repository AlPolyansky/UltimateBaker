ul.pagination
	li.pagination__item
		a.pagination__after(href="#").fa.fa-chevron-left 
	li.pagination__item
		a.pagination__link(href="#") 1
	li.pagination__item.active
		a.pagination__link(href="#") 2
	li.pagination__item
		a.pagination__link(href="#") 3
	li.pagination__item
		a.pagination__more(href="#") ...
	li.pagination__item
		a.pagination__link(href="#") 10
	li.pagination__item
		a.pagination__before(href="#").fa.fa-chevron-right 